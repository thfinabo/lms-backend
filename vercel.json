{
    "version": 2,
    "installs": [
        {
          "src": "./",
          "use": "@verce/npm"
        }
      ],
    "builds": [
        {
          "src": "index.js",
          "use": "@vercel/node"
        }
      ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ]
  }