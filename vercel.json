{
    "version": 2,
    "installCommand": [
        {
            "src": "/(.)",
            "use": "npm install"
        }
    ],
    "builds": [
        {
          "src": "index.js",
          "use": "@vercel/node"
        }
      ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ]
  }